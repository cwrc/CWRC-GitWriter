{
  "sourceType": "unambiguous",
  "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "entry",
          "modules": false,
          "corejs": { 
            "version": 3,
            "proposals": true
          }
        }
      ],
      "@babel/preset-react"
    ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-transform-react-jsx",
    [
      "@babel/plugin-transform-runtime",
      {
        "absoluteRuntime": false,
        "corejs": {
          "version": 3,
          "proposals": true
        },
        "helpers": true,
        "regenerator": true,
        "version": "^7.14.7"
      }
    ]
  ]
}
