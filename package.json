{
	"name": "cwrc-git-writer",
	"version": "0.0.0",
	"description": "xml editing and rdf annotation using github as document store",
	"keywords": [
		"xml",
		"rdf",
		"editor",
		"annotation"
	],
	"homepage": "https://github.com/cwrc/CWRC-GitWriter",
	"bugs": {
		"url": "https://github.com/cwrc/CWRC-GitWriter/issues"
	},
	"license": "GPL-2.0",
	"contributors": [
		"Andrew MacDonald <andrewjames.code@gmail.com>",
		"James Chartrand <jc.chartrand@gmail.com>",
		"Luciano Frizzera <lucaju@gmail.com>"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/cwrc/CWRC-GitWriter.git"
	},
	"scripts": {
		"start": "env-cmd -e production node ./server/server.js",
		"dev:middleware-server-dev": "env-cmd -e development nodemon -L ./server/index.mjs",
		"dev:webpack": "env-cmd -e development npm run cwrc-writer-base:dev-editor-css & webpack --config webpack.dev.js ",
		"dev:webpack-watch": "env-cmd -e development npm run cwrc-writer-base:dev-editor-css & npm run dev:worker-validator & webpack --config webpack.dev.js --watch",
		"dev:worker-validator": "cd \"node_modules/cwrc-writer-base/node_modules/cwrc-worker-validator\" && npm run build-dev",
		"cwrc-writer-base:dev-editor-css": "cd \"node_modules/cwrc-writer-base\" && npm run build-editor-css-dev",
		"cwrc-writer-base:build": "cd \"node_modules/cwrc-writer-base\" && npm run build",
		"build": "npm run cwrc-writer-base:build && npm run build:webpack-prod && npm run build:worker-validator && npm run mockup-ui:copy-files",
		"build:webpack-prod": "webpack --config webpack.prod.js",
		"build:worker-validator": "cd \"node_modules/cwrc-writer-base/node_modules/cwrc-worker-validator\" && npm run build",
		"stats": "webpack --profile --json > stats.json",
		"analyzer": "webpack-bundle-analyzer stats.json",
		"mockup-ui:copy-files": "copyfiles -u 1 mockup-ui/* mockup-ui/**/* dist/mockup-ui",
		"cm": "git-cz"
	},
	"dependencies": {
		"cwrc-git-dialogs": "github:cwrc/cwrc-git-dialogs#development",
		"cwrc-public-entity-dialogs": "^3.1.0",
		"cwrc-writer-base": "github:cwrc/CWRC-WriterBase#worker-validator",
		"dbpedia-entity-lookup": "github:cwrc/dbpedia-entity-lookup#development",
		"env-cmd": "^10.1.0",
		"express": "^4.17.1",
		"geonames-entity-lookup": "github:cwrc/geonames-entity-lookup#development",
		"getty-entity-lookup": "github:cwrc/getty-entity-lookup#development",
		"lgpn-entity-lookup": "github:cwrc/lgpn-entity-lookup#development",
		"link": "^0.1.5",
		"viaf-entity-lookup": "github:cwrc/viaf-entity-lookup#development",
		"wikidata-entity-lookup": "github:cwrc/wikidata-entity-lookup#development"
	},
	"devDependencies": {
		"@babel/core": "^7.14.3",
		"@babel/eslint-parser": "^7.14.3",
		"@babel/plugin-proposal-class-properties": "^7.13.0",
		"@babel/plugin-proposal-private-methods": "^7.13.0",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-react-jsx": "^7.14.3",
		"@babel/plugin-transform-runtime": "^7.14.3",
		"@babel/preset-env": "^7.14.2",
		"@babel/preset-react": "^7.13.13",
		"@babel/runtime-corejs3": "^7.14.0",
		"babel-loader": "^8.2.2",
		"babel-loader-exclude-node-modules-except": "^1.1.2",
		"clean-webpack-plugin": "^4.0.0-alpha.0",
		"commitizen": "^4.2.4",
		"console-browserify": "^1.2.0",
		"copy-webpack-plugin": "^9.0.0",
		"copyfiles": "^2.4.1",
		"css-loader": "^5.2.6",
		"css-minimizer-webpack-plugin": "^3.0.0",
		"eslint": "^7.27.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^3.4.0",
		"file-loader": "^6.2.0",
		"git-cz": "^4.7.6",
		"html-webpack-plugin": "^5.3.1",
		"less": "^4.1.1",
		"less-loader": "^9.0.0",
		"less-watch-compiler": "^1.15.1",
		"mini-css-extract-plugin": "^1.6.0",
		"nodemon": "^2.0.7",
		"prettier": "^2.3.0",
		"style-loader": "^2.0.0",
		"svg-inline-loader": "^0.8.2",
		"terser-webpack-plugin": "^5.1.2",
		"ts-loader": "^9.2.2",
		"typescript": "^4.3.2",
		"webpack": "^5.37.1",
		"webpack-bundle-analyzer": "^4.4.2",
		"webpack-cli": "^4.7.0",
		"webpack-dev-middleware": "^4.3.0",
		"webpack-dev-server": "^3.11.2",
		"webpack-hot-middleware": "^2.25.0",
		"webpack-merge": "^5.7.3",
		"webpackbar": "^5.0.0-3"
	}
}
